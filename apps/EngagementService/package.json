    "react-dom": "^19.2.0",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "concurrently": "^9.1.0",
    "eslint": "^9.39.1",
    "typescript": "5.9.2"
  }
